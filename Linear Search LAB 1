#include <iostream>
using namespace std;

int main()
{
    int n = 0;
    cout<<"Lakshyajit Thapa" << endl << "A2305220281" << endl;
    cout << "Enter the number of elements:";
    cin >> n;
    int a[n];
    
    for (int i=0;i<n;i++)
    {
        cout << "Enter the value of element " << i+1 << endl;
        cin >> a[i];
    }
    
    cout << "Enter the element you want to find:" << endl;
    int find;
    cin >> find;
    int found = 0;
    int fw;
    for (int i=0;i<=n;i++)
    {
        if (a[i]==find)
        {
            found = 1;
            fw = i;
        }
    }
    if (found == 1)
    {
        cout << "Element found at index:" << fw+1; 
    }
    else
    {
        cout << "Element not found";
    }
}
    
